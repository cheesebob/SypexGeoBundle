{
    "name": "alexeyfedotof/sypex-geo-bundle",
    "description": "An adaptation of SypexGeo library to Symfony Bundle system",
    "keywords": [
        "sypexgeo", "position", "ip location", "geoip"
    ],
    "homepage": "https://github.com/yamilovs/SypexGeoBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yamilov Stepan",
            "email": "yamilovs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yamilovs\\Bundle\\SypexGeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yamilovs\\Bundle\\SypexGeoBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",        
        "guzzlehttp/guzzle": "^6.0",
        "symfony/framework-bundle": "6.*",
        "yamilovs/sypex-geo": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "symfony/console": "^3.4|^4.2"
    },
    "config": {
        "sort-packages": true
    }
}
